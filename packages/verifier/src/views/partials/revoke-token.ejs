<div id="revoke-token-container" class="content-container">
  <p id="revoke-masked-token" class="page-title"></p>
  <p>
    <input
      type="text"
      placeholder="Full API Token"
      id="revoke-token-input"
      class="content-input"
    />
  </p>
  <p>
    <input
      type="text"
      placeholder="Reason (Max 200 char)"
      id="revoke-reason-input"
      class="content-input"
    />
  </p>
  <button id="revoke-token-button" class="action-button center-button">
    Revoke Token
  </button>
  <p id="revoke-token-result" class="padding-5 text-center"></p>
  <p id="revoke-token-error" class="error"></p>
  <button id="revoke-back-button" class="action-button center-button">
    Back
  </button>
</div>
